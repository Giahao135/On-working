--Notify if character is wrong
Hero = game.Players.LocalPlayer.Character:GetAttribute("Character")
if Hero ~= "Bald" then
    game.StarterGui:SetCore("SendNotification", {
        Title = "Dumbass brochacho did not know";
        Text = "Use Saitama(The Strongest Hero) to unseal Gojo";
        Duration = 5;
    })
    return
end

local Animation = 106400765698758
local StartTime = 0 -- what duration it starts like video
local Speed = 1 -- animation speed
local Duration= 6.98 -- how much till the animation stops

local a = Instance.new("Animation")
a.AnimationId = "rbxassetid://" .. Animation
local p = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(a)
p:Play()
p.TimePosition = StartTime -- what duration it starts like video
p:AdjustSpeed(0.3)
delay(Duration, function() -- delete these 3 lines if you don't want to stop it or you can adjust 7 to less or longer to stop animation 
p:Stop()
end)

task.spawn(function()
    local function GetGitSound(GithubSnd, SoundName)
        local url = GithubSnd
        if not isfile(SoundName .. ".mp3") then
            writefile(SoundName .. ".mp3", game:HttpGet(url))
        end
        local sound = Instance.new("Sound")
        sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
        return sound
    end

    local Clock = GetGitSound("https://github.com/Giahao135/Audio/blob/main/Screenrecorder-2025-08-06-15-01-07-6_06082025.mp3?raw=true", "Againz")

    Clock.Parent = workspace
    Clock.Volume = 0 -- start at 0 for fade-in
    Clock.TimePosition = 30
    Clock:Play()

    -- Fade-in logic
    local targetVolume = 15
    local fadeInDuration = 2 -- fade-in duration in seconds
    local fadeInStep = 0.1
    for i = 1, fadeInDuration / fadeInStep do
        Clock.Volume = (i / (fadeInDuration / fadeInStep)) * targetVolume
        task.wait(fadeInStep)
    end

    Clock.Volume = targetVolume -- ensure it hits max volume

    -- Wait before starting the fade-out
    task.wait(7)

    -- Fade-out logic
    local fadeOutDuration = 5
    local fadeOutStep = 0.1
    for i = 1, fadeOutDuration / fadeOutStep do
        Clock.Volume = Clock.Volume - (targetVolume / (fadeOutDuration / fadeOutStep))
        task.wait(fadeOutStep)
    end

    Clock:Stop()
    Clock:Destroy()
end)

local player = game.Players.LocalPlayer
local camera = game.Workspace.CurrentCamera
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local head = character:WaitForChild("Head")

-- Save the current camera type and subject
local originalCameraType = camera.CameraType
local originalCameraSubject = camera.CameraSubject

-- Set the camera to Scriptable mode
camera.CameraType = Enum.CameraType.Scriptable

-- Set starting CFrame in front of the head
local startDistance = 50 -- how far in front the camera starts
local lookDirection = head.CFrame.LookVector -- forward direction
local startPosition = head.Position + (lookDirection * -startDistance) -- in front of face
local startCFrame = CFrame.new(startPosition, head.Position)

-- Set ending CFrame just in front of the head
local endOffset = lookDirection * -5 -- 5 studs in front of face
local endCFrame = CFrame.new(head.Position + endOffset, head.Position)

-- Set the initial camera position
camera.CFrame = startCFrame

-- Tween to zoom in
local TweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(7, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)

local tween = TweenService:Create(camera, tweenInfo, {CFrame = endCFrame})
tween:Play()
tween.Completed:Wait()

-- Pause briefly at the zoomed-in point

local Animation = 106400765698758
local StartTime = 2.22 -- what duration it starts like video
local Speed = 1 -- animation speed
local Duration= 1.3 -- how much till the animation stops

local a = Instance.new("Animation")
a.AnimationId = "rbxassetid://" .. Animation
local p = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(a)
p:Play()
p.TimePosition = StartTime -- what duration it starts like video
p:AdjustSpeed(1)
delay(Duration, function() -- delete these 3 lines if you don't want to stop it or you can adjust 7 to less or longer to stop animation 
p:Stop()
end)

wait(0.3)

-- Snap back to the player's view
camera.CFrame = humanoidRootPart.CFrame
camera.CameraType = originalCameraType
camera.CameraSubject = originalCameraSubject

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

wait(0)
local function impactframe()
    local light = Instance.new("Highlight", character)
    local cc = Instance.new("ColorCorrectionEffect", game.Lighting)
    light.OutlineTransparency = 1
    light.FillTransparency = 0
    light.FillColor = Color3.new(0, 0, 0)
    cc.Brightness = 1
    wait(0.1)
    light.FillColor = Color3.new(1, 1, 1)
    cc.Brightness = -1
    wait(0.1)
    cc:Destroy()
    light:Destroy()
end

impactframe()

