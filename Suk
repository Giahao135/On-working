local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Function to teleport the player forward
local function teleportPlayer()
    -- Make character invisible
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.LocalTransparencyModifier = 1 -- Client-side transparency
        end
    end

    -- Wait 1 second before teleporting
    task.wait(0.2)

    -- Teleport player forward
    local teleportDistance = 48 -- Change this for more distance
    local forwardVector = humanoidRootPart.CFrame.LookVector * teleportDistance
    humanoidRootPart.CFrame = humanoidRootPart.CFrame + forwardVector

    -- Wait 0.5 seconds before making visible again
    task.wait(0.2)

    -- Make character visible again
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.LocalTransparencyModifier = 0 -- Restore client-only visibility
        end
    end
end

-- Call the teleportation function
teleportPlayer()

task.wait(0)

local AnimAnim2 = Instance.new("Animation")

AnimAnim2.AnimationId = "rbxassetid://14920779925"

local Anim2 = Humanoid:LoadAnimation(AnimAnim2)


local startTime = 3.7


Anim:Stop()

Anim2:Play()

Anim2:AdjustSpeed(0)

