local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local function checkProximityAndHealth()
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            local otherHumanoid = otherPlayer.Character:FindFirstChild("Humanoid")
            local otherRoot = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
            if otherHumanoid and otherRoot and otherHumanoid.Health <= 20 then
                local distance = (humanoidRootPart.Position - otherRoot.Position).Magnitude
                if distance < 10 then
                    return true -- Low health target is nearby
                end
            end
        end
    end

    local npc = workspace.Live:FindFirstChild("Weakest Dummy")
    if npc then
        local npcHumanoid = npc:FindFirstChild("Humanoid")
        local npcRoot = npc:FindFirstChild("HumanoidRootPart")
        if npcHumanoid and npcRoot and npcHumanoid.Health <= 20 then
            local distance = (humanoidRootPart.Position - npcRoot.Position).Magnitude
            if distance < 10 then
                return true -- Low health NPC is nearby
            end
        end
    end
    return false -- No low health target is nearby
end

local function playSound()
    -- Only play sound if a low health target is nearby
    if not checkProximityAndHealth() then
        return
    end

    task.spawn(function()
        local function GetGitSound(GithubSnd, SoundName)
            local url = GithubSnd
            if not isfile(SoundName .. ".mp3") then
                writefile(SoundName .. ".mp3", game:HttpGet(url))
            end
            local sound = Instance.new("Sound")
            sound.SoundId = (getcustomasset or getsynasset)(SoundName .. ".mp3")
            return sound
        end

        local Clock = GetGitSound("https://github.com/Giahao135/Audio/blob/main/REMEMBERJJK2.mp3?raw=true", "REMEMBER2")

        Clock.Parent = workspace
        Clock.Volume = 4
        Clock.TimePosition = 2.1
        Clock:Play()

        -- Fade-out logic
        local fadeOutDuration = 7 -- duration in seconds for the fade-out
        local fadeOutStep = 0.1 -- time in seconds between each volume decrease
        task.wait(2) -- Wait before starting the fade-out (optional)

        for i = 1, fadeOutDuration / fadeOutStep do
            Clock.Volume = Clock.Volume - (16 / (fadeOutDuration / fadeOutStep)) -- Gradually reduce volume
            task.wait(fadeOutStep)
        end

        Clock:Stop()
        Clock:Destroy()
    end)
end

playSound() -- Only executes if a low health target is close

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local triggered = false

local function impactframe()
    if triggered then return end
    triggered = true

    local light = Instance.new("Highlight", character)
    local cc = Instance.new("ColorCorrectionEffect", game.Lighting)
    light.OutlineTransparency = 1
    light.FillTransparency = 0
    light.FillColor = Color3.new(0, 0, 0)
    cc.Brightness = 1

    wait(0.05)
    light.FillColor = Color3.new(1, 1, 1)
    cc.Brightness = -1
    wait(0.05)

    cc:Destroy()
    light:Destroy()
end

local function checkProximity()
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            local otherRoot = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
            if otherRoot then
                local distance = (humanoidRootPart.Position - otherRoot.Position).Magnitude
                if distance < 10 then
                    return true -- Indicate a target is in proximity
                end
            end
        end
    end

    local npc = workspace.Live:FindFirstChild("Weakest Dummy")
    if npc then
        local npcRoot = npc:FindFirstChild("HumanoidRootPart")
        if npcRoot then
            local distance = (humanoidRootPart.Position - npcRoot.Position).Magnitude
            if distance < 10 then
                return true -- Indicate a target is in proximity
            end
        end
    end
    return false -- Indicate no target is in proximity
end

local function executeImpact()
    if checkProximity() then
        impactframe()
    end
end

executeImpact() -- Only check proximity once when script executes


delay(0.7, function()

    Anim:Stop()

end)


    end

end

